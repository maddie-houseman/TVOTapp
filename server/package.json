{
    "name": "server",
    "private": true,
    "type": "module",
    "scripts": {
        "dev": "tsx watch src/index.ts",
        "build": "tsc -p tsconfig.json",
        "start": "sh -lc 'node dist/index.js || node dist/src/index.js'",
        "seed": "node prisma/seed.js",
        "seed-tbm": "tsx src/scripts/seed-tbm.ts",
        "migrate-to-tbm": "tsx src/scripts/migrate-to-tbm.ts",
        "allocation-pipeline": "tsx src/scripts/allocation-pipeline.ts",
        "postinstall": "prisma generate",
        "migrate": "prisma migrate deploy",
        "deploy": "prisma db push && npm run seed",
        "setup-tbm": "npm run seed-tbm && npm run migrate-to-tbm && npm run allocation-pipeline"
    },
    "prisma": {
        "seed": "node prisma/seed.js"
    },
    "dependencies": {
        "@prisma/client": "^6.15.0",
        "bcrypt": "^6.0.0",
        "bcryptjs": "^3.0.2",
        "cookie-parser": "^1.4.6",
        "cors": "^2.8.5",
        "docx": "^9.5.1",
        "dotenv": "^16.4.5",
        "express": "^4.21.2",
        "express-rate-limit": "^8.1.0",
        "jsonwebtoken": "^9.0.2",
        "openai": "^4.55.0",
        "puppeteer": "^24.22.3",
        "zod": "^3.25.76"
    },
    "devDependencies": {
        "@types/bcryptjs": "^2.4.6",
        "@types/cookie-parser": "^1.4.7",
        "@types/cors": "^2.8.17",
        "@types/express": "^4.17.21",
        "@types/jsonwebtoken": "^9.0.6",
        "@types/node": "^20.19.17",
        "prisma": "^6.15.0",
        "tsx": "^4.7.0",
        "typescript": "^5.4.5"
    }
}
